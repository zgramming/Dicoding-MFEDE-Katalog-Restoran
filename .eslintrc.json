{
    "plugins": [
        "codeceptjs"
    ],
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true,
        "codeceptjs/codeceptjs": true  
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "object-curly-newline": "off"
    }
}
